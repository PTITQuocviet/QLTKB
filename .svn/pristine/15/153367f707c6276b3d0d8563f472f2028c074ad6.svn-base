package model.web;

import jakarta.ws.rs.QueryParam;

/**
 * @author toanlh-10
 */
public class SearchParam {

    /**
     * index của phần tử đầu tiên của kết quả trả về trong danh sách kết quả tìm kiếm
     */
    @QueryParam("start")
    private Integer start;
    /**
     * số lượng dòng tối đa của kết quả trả về
     */
    @QueryParam("rows")
    private Integer rows;

    public Integer getStart() {
        return start;
    }

    public void setStart(Integer start) {
        this.start = start;
    }

    public Integer getRows() {
        return rows;
    }

    public void setRows(Integer rows) {
        this.rows = rows;
    }
}
