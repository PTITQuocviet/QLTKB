nav.navbar.navbar-expand-lg.navbar-light {
    border: solid 1px #F6F6F6;
}

.dropdown-menu.show {
    display: block;
    margin-top: 16px;
    /* width: 101%; */
    width: 100%;
}

button.btn.new-project {
    border: 1px dashed #1890FF;
    box-sizing: border-box;
    font-family: Helvetica;
    font-style: normal;
    font-weight: normal;
    font-size: 14px;
    line-height: 22px;
    color: #1890FF;
}

button#dropdownBasic1 {
    background: #0043A5;
    color: #FFFFFF
}

.btn-group.dropdown {
    margin-left: 20px;
}

p.text {
    text-align: -webkit-center;
    min-width: 100px;
    margin-bottom: 0%;
    margin-block: auto;
}

img.person {
    margin-right: 15px;
}

button.btn-notice {
    border: none;
    background-color: #FFFFFF
}

.ring {
